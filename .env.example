# OpenAI API Key (Required for backend)
OPENAI_API_KEY=sk-your-openai-api-key-here

# Backend Configuration
SESSION_TTL_HOURS=2
MAX_FILE_SIZE_MB=100

# Production Scale: Intelligent Query Routing
# Automatically routes queries based on complexity to optimize cost
# - Simple queries: Template-based (free, instant)
# - Medium queries: GPT-3.5-turbo ($0.001 per query)
# - Complex queries: GPT-4 ($0.024 per query)
ENABLE_QUERY_ROUTING=true

# Frontend Configuration
VITE_API_URL=http://localhost:8000
